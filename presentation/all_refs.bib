@article{usamiFittingStructuralEquation2017,
  title = {Fitting {{Structural Equation Model Trees}} and {{Latent Growth Curve Mixture Models}} in {{Longitudinal Designs}}: {{The Influence}} of {{Model Misspecification}}},
